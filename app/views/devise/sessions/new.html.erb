<section id="log-in" class="logg">
  <div class="container logg-content">
    <div class="row">
      <div class="col-xs-12">
        <%= image_tag("logo-white.png", alt: "Logo PetaTime") %>
        <!-- <h3>Log in</h3> -->
        <div class="separate-line"></div>
          <div class="col-xs-6 text-center">
            <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
                <p><span class="color-white">Login.</span> Please enter your User ID.</p>
                <%= f.email_field :email, autofocus: true %>
                <p><span class="color-white">Password.</span> Please enter your Password.</p>
                <%= f.password_field :password, autocomplete: "off" %>
              <% if devise_mapping.rememberable? -%>
              <% end -%>
                <%= f.submit "Log in" %>
            <% end %>
            <%= render "devise/shared/links" %>
          </div>
          <div class="col-xs-6">
            <div id="txt" class=""></div>
            <div class="clearfix"></div>
          </div>
          <div class="clearfix"></div>
        <div class="separate-line"></div>
      </div>
    </div>
  </div>
</section>